{
  "name": "mapping-visualizer",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "{projectRoot}"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc && vite build",
        "cwd": "{projectRoot}"
      },
      "outputs": ["{projectRoot}/dist", "{projectRoot}/dist-ssr"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "{projectRoot}"
      }
    }
  }
}
